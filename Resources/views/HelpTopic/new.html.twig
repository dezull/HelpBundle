{% extends "DezullHelpBundle::admin_layout.html.twig" %}

{% block content %}

<h1>New Topic</h1>

<div id="contentInner">
    <form id="topicForm" action="{{ path('dezull_help_topic_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div class="clear"></div>
        <button type="submit">Create</button>
    </form>

</div>

<div id="actionBar">
    <a class="middle" href="{{ path('dezull_help_category_edit', { id: entity.category.id }) }}">Back to the list</a>
</div>

{% endblock %}
